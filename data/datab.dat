################################################
Molasse #
################################################
-start
1   k_Mola        = 1e-06
3   k_Nit         = 1e-04
8   v_up_max      = parm(1)/86400 #def=800*0.0093=7.44
10  mMola         = TOT("Molasse")
26  mCu           = TOT("Cu")
27  mNit          = TOT("N(5)")+TOT("N(3)")+TOT("N(-3)")
30  mBac      = mol("Bacteria")
32  if (mMola     < 5e-07) then goto 200
#36  theta_bio_max = 0.000060 # not too bad
36  theta_bio_max = 0.002 # 0.000075
37  inhib_bio     = (theta_bio_max-mBac)/theta_bio_max
40  mon_Mola      = mMola / (k_Mola + mMola)
50  mon_Nit       = mNit  / (k_Nit  + mNit)
#60  inhib_bio    = 1 #if no inhibition desired, remove #
80  growth        = v_up_max*mon_Mola*mon_Nit*mBac # *inhib_bio
100 rate          = growth
110 moles         = rate * time
200 SAVE moles
-end


