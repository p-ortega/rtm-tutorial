44  inhib_bio     = (theta_bio_max-mBac)/theta_bio_max
50  growth        = v_up_max*mon_Acet*mon_Nit*mBac # *inhib_bio
#50  rate = parm(1) / 86400 * mAcetate * (mAcet / (parm(2) + mAcet))
100 rate          = growth
110 moles         = rate * time
#110  moles = rate * m * (m/m0) * time
#150 if (moles > m) then moles = m
200 SAVE moles
-end


